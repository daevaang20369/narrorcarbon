<section class="hero">
    <div class="container">
        <div class="row">
            <span> <h1 style="color: black;">Last Three Submissions</h1></span>
            <div class="col-12 offset-md-1 col-md-11">
                <div class="swiper-container hero-slider">
                    <div class="swiper-wrapper">

                        <div class="swiper-slide slide-content d-flex align-items-center">
                            <div class="single-slide">
                                <h1 data-aos="fade-right" data-aos-delay="200">
                                    <span style="color: #F4A442;"><%= (resu)[0].releaseco %></span> KG <br>of carbon <br> released on
                                </h1>
                                <p data-aos="fade-right" data-aos-delay="600" class="mt-3">
                                    DATE :
                                    <%= (resu)[0].submitdate %>
                                </p>

                            </div>
                        </div>
                        <div class="swiper-slide slide-content d-flex align-items-center">
                            <div class="single-slide">
                                <h1 data-aos="fade-right" data-aos-delay="200">
                                    <span style="color: #F4A442;"><%= [1].releaseco %></span> KG <br> of carbon <br> released on
                                </h1>
                                <p data-aos="fade-right" data-aos-delay="600" class="mt-3">
                                    DATE :
                                    <%= Object.values(resu)[1].submitdate %>
                                </p>

                            </div>
                        </div>
                        <div class="swiper-slide slide-content d-flex align-items-center">
                            <div class="single-slide">
                                <h1 data-aos="fade-right" data-aos-delay="200">
                                    <span style="color: #F4A442;"><%= (resu)[2].releaseco %> </span> KG <br>of carbon <br> released on
                                </h1>
                                <p data-aos="fade-right" data-aos-delay="600" class="mt-3">
                                    DATE :
                                    <%= (resu)[2].submitdate %>
                                </p>

                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
        <!-- Add Control -->
        <span class="arr-left"><i class="fa fa-angle-left"></i></span>
        <span class="arr-right"><i class="fa fa-angle-right"></i></span>
    </div>
    <div class="texture"></div>
</section>


